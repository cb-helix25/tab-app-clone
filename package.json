    "scripts": {
        "dev:teamsfx": "concurrently \"npm run dev-tab:teamsfx\" \"npm run dev-api:teamsfx\" \"npm run start:server\" \"npm run start\"",
        "dev-tab:teamsfx": "env-cmd --silent -f .localConfigs npm run start",
        "dev-api:teamsfx": "cd api && npm run dev:teamsfx",
        "dev": "concurrently \"npm run start:server:watch\" \"npm start\"",
        "dev:all": "concurrently \"npm run start:azurite\" \"npm run start:decoupled-functions\" \"npm run start:api-functions\" \"npm run start:server\" \"npm run start:dev\"",
        "dev:backend": "concurrently \"npm run start:azurite\" \"npm run start:decoupled-functions\" \"npm run start:api-functions\" \"npm run start:server\"",
        "dev:functions": "concurrently \"npm run start:azurite\" \"npm run start:decoupled-functions\" \"npm run start:api-functions\"",
        "start": "node app.js",
        "start:client": "react-scripts start",
        "start:dev": "react-scripts start",
        "start:3001": "set PORT=3001 && react-scripts start",
        "start:quiet": "BROWSER=none react-scripts start",
        "start:server": "node app.js",
        "start:server:watch": "nodemon --watch server --watch app.js --ext js,ts,json --exec node app.js",
        "start:server:legacy": "node server/index.js",
        "start:azurite": "npx azurite --location ./azurite --debug ./azurite/debug.log --blobPort 10000 --queuePort 10001 --tablePort 10002",
        "start:decoupled-functions": "cd \"d:\\helix projects\\workspace\\tab apps\\helix hub v1\\decoupled-functions\" && func start --port 7071",
        "start:api-functions": "cd \"d:\\helix projects\\workspace\\tab apps\\helix hub v1\\api\" && func start --port 7072",
        "build": "cross-env CI=false react-scripts build",
        "build:azure": "npm run build && echo Build completed for Azure deployment",
        "postinstall": "echo 'Dependencies installed successfully'",
        "test": "jest",
        "eject": "react-scripts eject",
        "generate:attendance": "node scripts/generateLocalAttendance.js",
        "generate:risk": "node scripts/generateLocalRiskAssessments.js",
        "generate:tiller": "node scripts/generateLocalTillerResponses.js",
        "generate:snippets": "node scripts/generateLocalSnippetEdits.js"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "homepage": "."
}
